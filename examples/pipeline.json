{
  "commands": [
    {
      "command": "layout",
      "inputFile": "examples/data/100nodes.json",
      "outputFile": "output/100nodes_layout.json",
      "formatOutput": true,
      "options": {
        "algorithm": "d3-force",
        "numTicks": 100,
        "layoutKey": "layout"
      }
    },
    {
      "command": "style",
      "inputFile": "output/100nodes_layout.json",
      "outputFile": "output/100nodes_layout_styled.json",
      "formatOutput": true,
      "options": {
        "colorKey": "genre_like",
        "colorScheme": {
          "type": "valueMap",
          "valueMap": {
            "Comedy": "purple",
            "Action": "red"
          },
          "defaultColor": "#ccc"
        }
      }
    },
    {
      "command": "draw",
      "inputFile": "output/100nodes_layout_styled.json",
      "options": {
        "imageOutput": "output/100nodes.png",
        "width": 600,
        "height": 600
      }
    }
  ]
}
